<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="0">

    <record model="ir.module.category" id="hr_hospital.module_category_hospital">
        <field name="description">Helps you handle your hospital needs</field>
        <field name="sequence">7</field>
        <field name="name">Hospital</field>
    </record>

    <record id="group_hospital_manager" model="res.groups">
        <field name="name">Hospital Administrator</field>
        <field name="category_id" ref="hr_hospital.module_category_hospital"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>

    <record id="group_hospital_user" model="res.groups">
        <field name="name">Hospital Users</field>
        <field name="category_id" ref="hr_hospital.module_category_hospital"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>

</odoo>
